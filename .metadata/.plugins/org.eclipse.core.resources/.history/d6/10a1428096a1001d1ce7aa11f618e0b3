package main;

public class MainClass {
	public static void main(String args[]) {
		BkashAccount uOne = new BkashAccount();
		BkashAccount uTwo = new BkashAccount();
		
		uOne.name = "Farhad";
		uOne.balance = 500;
		uTwo.name = "Foysal";
		uTwo.balance = 500;
		
		uOne.SendMoney(uTwo, 600);
		uTwo.SendMoney(uOne, 600);

	}
}
